<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SlashLayout">
        <attr name="slash_heightDiff" format="dimension"/>
        <attr name="slash_cornerRadius" format="dimension"/>
        <attr name="slash_shadowSize" format="dimension"/>
        <attr name="slash_shadowDepth" format="float"/>
        <attr name="slash_backgroundColor" format="color"/>
        <attr name="slash_eatTouchEvent" format="boolean"/>
        <attr name="slash_autoTopPadding" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="UpDownArrow">
        <attr name="upa_arrowWidth" format="dimension"/>
        <attr name="upa_arrowHeight" format="dimension"/>
        <attr name="upa_arrowThickness" format="dimension"/>
        <attr name="upa_arrowColor" format="color"/>
        <attr name="upa_arrowCurvature" format="float"/>
        <attr name="upa_breakStart" format="float"/>
        <attr name="upa_breakEnd" format="float"/>
        <attr name="upa_progress" format="float"/>
        <attr name="upa_isUpToDown" format="boolean"/>
        <attr name="upa_fitToViewSize" format="boolean"/>
        <attr name="upa_skipBreakTime" format="boolean"/>
        <attr name="upa_richDrawing" format="boolean"/>
        <attr name="upa_mode" format="enum">
            <enum name="flip" value="0"/>
            <enum name="cross" value="1"/>
        </attr>
    </declare-styleable>
    
    <declare-styleable name="WhackAMoleBehavior">
        <attr name="wam_anchorBottomSheet" format="reference"/>
        <attr name="wam_detectionRange" format="dimension"/>
        <attr name="wam_revealFromLeftSide" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="AdvancedBottomSheetBehavior">
        <attr name="absb_belowOfOnExpanded" format="reference"/>
        <attr name="absb_belowOfOnCollapsed" format="reference"/>
        <attr name="absb_fadeOutSheetOnDragging" format="boolean"/>
        <attr name="absb_enableOverDragging" format="boolean"/>
        <attr name="absb_collapsedSheetAlpha" format="fraction"/>
        <attr name="absb_defaultState" format="enum">
            <enum name="expanded" value="3"/>
            <enum name="collapsed" value="4"/>
            <enum name="hidden" value="5"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="BottomSheetFamilyLayout">
        <attr name="layout_hostBottomSheet" format="reference"/>
    </declare-styleable>

    <declare-styleable name="LinkageValueBehavior">
        <attr name="behavior_valueOnExpanded" format="float"/>
        <attr name="behavior_valueOnCollapsed" format="float"/>
        <attr name="behavior_valueOnHidden" format="float"/>
        <attr name="behavior_valueInterpolator" format="enum">
            <enum name="linear" value="1"/>
            <enum name="linear_out_slow_in" value="2"/>
            <enum name="accelerate" value="3"/>
            <enum name="decelerate" value="4"/>
            <enum name="accelerate_decelerate" value="5"/>
            <enum name="anticipate" value="6"/>
            <enum name="anticipate_overshoot" value="7"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="LinkageTranslationBehavior">
        <attr name="behavior_pivot" format="float"/>
        <attr name="behavior_valueTypeOnExpanded" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_size" value="1"/>
            <enum name="relative_to_parent_size" value="2"/>
        </attr>
        <attr name="behavior_valueTypeOnCollapsed" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_size" value="1"/>
            <enum name="relative_to_parent_size" value="2"/>
        </attr>
        <attr name="behavior_valueTypeOnHidden" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_size" value="1"/>
            <enum name="relative_to_parent_size" value="2"/>
        </attr>
        <attr name="behavior_coordinateTypeOnExpanded" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_position" value="1"/>
        </attr>
        <attr name="behavior_coordinateTypeOnCollapsed" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_position" value="1"/>
        </attr>
        <attr name="behavior_coordinateTypeOnHidden" format="enum">
            <enum name="absolute" value="0"/>
            <enum name="relative_to_self_position" value="1"/>
        </attr>
    </declare-styleable>
    
    <declare-styleable name="LinkageSlideInOutBehavior">
        <attr name="behavior_slideInOutFromLeftSide" format="boolean"/>
    </declare-styleable>

</resources>
